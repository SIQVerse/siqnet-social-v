├── siqnet_backend/              # Django project root
│   ├── __init__.py
│   ├── settings.py
│   ├── urls.py
│   ├── wsgi.py
│   ├── asgi.py

├── src/                         # React frontend source code
│   ├── App.js
│   ├── App.css
│   ├── App.test.js
│   ├── index.js
│   ├── index.css
│   ├── logo.svg
│   ├── reportWebVitals.js
│   ├── setupTests.js
│   └── components/              # (Create this folder if needed for reusable components)

├── public/                      # React public assets
│   ├── index.html
│   ├── favicon.ico
│   ├── manifest.json
│   └── robots.txt

├── manage.py                    # Django CLI entry point
├── requirements.txt             # Python dependencies
├── runtime.txt                  # Python version for Railway
├── Procfile                     # Gunicorn entry point
├── .env                         # Environment variables
├── .gitignore-new               # Temporary ignore file (rename to .gitignore)
├── package.json                 # React dependencies
├── package-lock.json
├── README.md                    # Project documentation
